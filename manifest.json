{
  "name": "Your AdBlocker （自分専用広告ブロッカー）",
  "description": "Removes website ads by targeting and removing their HTML elements specified by class or ID. Blocks only the ads you want to block. ",
  "version": "1.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "deleteads.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_popup": "pop.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}